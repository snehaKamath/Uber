<body ng-app = "uberApp">
    <div class = "header">
     
    </div>
 <hr class = "verticalLine"/>
 <div class="row">
    <br>
  <div class="col-md-3"> 
      
 <ul class="nav nav-pills nav-stacked">
     
  <li role="presentation"><a ui-sref="customerProfile"><center>Profile</center></a></li>
  <li role="presentation" class="active"><a ui-sref="customerTrips"><center>My Trips</center></a></li>
  <li role="presentation"><a ui-sref="createRide"><center>Create Ride</center></a></li>
</ul>
      
      
</div>
      
 
 <div class="col-md-9">
     
<div ui-view></div>
 </div>
   
</div>
    
