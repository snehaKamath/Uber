<div class="row">
<button type="button"  class="btn btn-danger" style="width:100%;background-color: black;color: #fff;border-color: black;" ng-click=getPerCustomerGraph() >View graph for rides per Customer</button><br><br>
  <div id="cust" ng-if="perCustomer">
<input type="text" class="form-control" id="ssn_id_1" maxlength="3" placeholder="123" style="width:25%;display:inline;" ng-model="$parent.ssn_id_1"> -
			<input type="text" class="form-control" id="ssn_id_2" maxlength="2" placeholder="12" style="width:20%;display:inline;" ng-model="$parent.ssn_id_2"> - 
			<input type="text" class="form-control" id="ssn_id_3" maxlength="4" placeholder="1234(SSN Number)" style="width:47%;display:inline;" ng-model="$parent.ssn_id_3">
		<br><br><br>
		<div ng-if = "invalidSSN" class="alert alert-danger">{{invalidSSNmessage}}</div>
	<button class="btn btn-danger" style="background-color: black;color: #fff;border-color: black;" ng-click="viewPerCustomerGraph()" >Submit</button>
		  
		</div>
		<div  id="graph" ng-if="loadGraph_2">Loading graph...</div>
<div id="graph2"></div>
<div id="line2"></div>
<div ng-if="loadGraph2" >
<div id="cu" customer></div>
</div>
</div>

<div class="row">
<button type="button" class="btn btn-danger"  style="width:100%;background-color: black;color: #fff;border-color: black;" ng-click=getPerDriverGraph() >View graph for rides per driver</button><br><br>
  <div id="dri" ng-if="perDriver">
<input type="text" class="form-control" id="ssn_id_1" maxlength="3" placeholder="123" style="width:25%;display:inline;" ng-model="$parent.ssn_id_1"> -
			<input type="text" class="form-control" id="ssn_id_2" maxlength="2" placeholder="12" style="width:20%;display:inline;" ng-model="$parent.ssn_id_2"> - 
			<input type="text" class="form-control" id="ssn_id_3" maxlength="4" placeholder="1234(SSN Number)" style="width:47%;display:inline;" ng-model="$parent.ssn_id_3">
		<br><br><br>
		<div ng-if = "invalidSSN" class="alert alert-danger">{{invalidSSNmessage}}</div>
<button class="btn btn-danger" style="background-color: black;color: #fff;border-color: black;" ng-click="viewPerDriverGraph()" >Submit</button>
		  
		</div>
		<div  id="graph" ng-if="loadGraph_3">Loading graph...</div>
<div id="graph3"></div>
<div id="line3"></div>
<div ng-if="loadGraph3" >
<div driver></div>
</div>
</div>
