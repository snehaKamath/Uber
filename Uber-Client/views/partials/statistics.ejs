<div ng-controller="adminGraphsController">
 <div class="row"  >

<button type="button"  class="btn btn-danger" style="width:100%;background-color: black;color: #fff;border-color: black;" ng-click=getRevenueGraph() > View graph for total revenue per day</button><br><br>

<div  id="graph" ng-if="loadGraph">Loading graph...</div>
<div id="graph1"></div>
<div id="line1"></div>
<div ng-if="loadGraph1" >
<div revenue></div>
</div>
</div>

<div class="row"  >
<button type="button" class="btn btn-danger"  style="width:100%;background-color: black;color: #fff;border-color: black;" ng-click=getPerZipCodeGraph() >View graph for rides per Zipcode</button><br><br>
<div id="zipcode" ng-if="zipCode">
<input type="text" class="form-control" id="driver_zip_1" style="width:47%;display:inline;" maxlength="5" placeholder="Zip Code(XXXXX)" ng-model="$parent.zip_1">&nbsp;&nbsp;-
<div ng-if = "invalidZipCode" class="alert alert-danger">{{invalidZipCodeMessage}}</div>
<button class="btn btn-danger" style="background-color: black;color: #fff;border-color: black;" ng-click="viewPerZipCodeGraph()" >Submit</button>
<div  id="graph" ng-if="loadGraph_4">Loading graph...</div>
<div id="graph4"></div>
<div id="line4"></div>
<div style="position:relative;top:40px" id="g4" ng-if="loadGraph4"  zipcode></div>
</div>
</div>

